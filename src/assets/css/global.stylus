html, body, #app
    height 100%
    margin 0
    min-width 1366px
.el-breadcrumb
    margin-bottom 15px
    font-size 14px
.el-card
    box-shadow 0 1px 1px rgba(0, 0, 0, 0.15) !important
.el-pagination
    margin-top 12px
.el-cascader-menu
    height 300px
    .el-cascader-menu__wrap
        overflow-x hidden
.el-steps
